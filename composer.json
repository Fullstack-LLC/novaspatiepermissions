{
    "name": "fullstackllc/novaspatiepermissions",
    "description": "Laravel Nova tool for managing spaties roles/permissions in laravel's nova package.",
    "keywords": [
        "laravel",
        "nova",
        "spatie",
        "permissions"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "spatie/laravel-permission": "^6.4",
        "laravel/nova": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Fullstackllc\\Novaspatiepermissions\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fullstackllc\\Novaspatiepermissions\\ToolServiceProvider",
                "Fullstackllc\\Novaspatiepermissions\\NovaSpatiePermissionsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
